---
import Encrypt from "./Encrypt.astro";

export interface Props {
  password?: string;
  passwordSubtitle?: string;
}

const { password, passwordSubtitle } = Astro.props;
---

{
  !password ? (
    <slot />
  ) : (
    <Encrypt password={password} passwordSubtitle={passwordSubtitle}>
      <slot />
    </Encrypt>
  )
}
